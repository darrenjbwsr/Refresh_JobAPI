generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id        String  @id @default(uuid())
  title     String
  company   String
  location  String
  salary    Int?
  postedAt  DateTime @default(now())
}
